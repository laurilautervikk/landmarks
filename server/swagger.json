{
  "openapi": "3.0.0",
  "info": {
    "title": "Landmarks API",
    "description": "Documentation for Landmarks API",
    "contact": {},
    "version": "0.2"
  },
  "servers": [
    {
      "url": "http://localhost:8080/",
      "variables": {}
    }
  ],
  "paths": {
    "/api/get-landmarks": {
      "get": {
        "tags": ["Landmarks"],
        "summary": "Get a page of Landmarks",
        "description": "This endpoint returns a page of landmarks based on the parameters provided",
        "operationId": "ApiGetLandmarks_GET",
        "parameters": [
          {
            "name": "limit",
            "in": "query",
            "description": "Amount of records per page",
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 8
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "Page number to display",
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 1
            }
          },
          {
            "name": "searchFor",
            "in": "query",
            "description": "Filter the records with a search string",
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "headers": {},
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/landmarks_page_schema"
                }
              }
            }
          },
          "5XX": {
            "description": "Unexpected error."
          }
        },
        "deprecated": false
      }
    },
    "/api/get-landmark/{id}": {
      "get": {
        "tags": ["Landmarks"],
        "summary": "Returns a landmark",
        "description": "Returns a landmark object by ID",
        "operationId": "ReturnsalandmarkbyID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of landmark",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "61c1e8a132c709508ca09ca8"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "headers": {},
            "content": {
              "application/json; charset=utf-8": {
                "schema": {
                  "$ref": "#/components/schemas/landmark_schema"
                }
              }
            }
          },
          "5XX": {
            "description": "Unexpected error."
          }
        },
        "deprecated": false
      }
    },
    "/api/add-landmark": {
      "post": {
        "tags": ["Landmarks"],
        "summary": "Add Landmark",
        "description": "Add a landmark",
        "operationId": "ApiAddLandmark_POST",
        "parameters": [],
        "security": {
          "BearerAuth": []
        },
        "requestBody": {
          "description": "The object containing a new landmark passed in the body",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/landmark_schema"
              }
            }
          },
          "required": false
        },
        "responses": {
          "200": {
            "description": "Addition sucessful",
            "headers": {},
            "content": {}
          },
          "5XX": {
            "description": "Unexpected error."
          }
        },
        "deprecated": false
      }
    },
    "/api/add-comment/{id}": {
      "post": {
        "tags": ["Landmarks"],
        "summary": "Add Comment",
        "description": "Add a comment to a landmark",
        "operationId": "ApiAddComment_POST",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "61c1e8a132c709508ca09ca8"
          }
        ],
        "security": {
          "BearerAuth": []
        },
        "requestBody": {
          "description": "Comment data in request body",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/comment_schema"
              }
            }
          },
          "required": false
        },
        "responses": {
          "200": {
            "description": "Comment Added",
            "headers": {},
            "content": {}
          }
        },
        "5XX": {
          "description": "Unexpected error."
        },
        "deprecated": false
      }
    },
    "/api/edit-landmark/{id}": {
      "patch": {
        "tags": ["Landmarks"],
        "summary": "Edit Landmark",
        "description": "Edit a landmark",
        "operationId": "ApiEditLandmark_PATCH",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "61c1e8a132c709508ca09ca8"
          }
        ],
        "security": {
          "BearerAuth": []
        },
        "requestBody": {
          "description": "Passing edited landmark data in request body",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/landmark_schema"
              }
            }
          },
          "required": false
        },
        "responses": {
          "200": {
            "description": "Edit successful",
            "headers": {},
            "content": {}
          }
        },
        "deprecated": false
      }
    },
    "/api/delete-landmark/{id}": {
      "delete": {
        "tags": ["Landmarks"],
        "summary": "Delete Landmark",
        "description": "Delete a landmark",
        "operationId": "ApiDeleteLandmark_DELETE",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "61c1e8a132c709508ca09ca8"
          }
        ],
        "security": {
          "BearerAuth": []
        },
        "responses": {
          "200": {
            "description": "OK",
            "headers": {},
            "content": {}
          },
          "5XX": {
            "description": "Unexpected error."
          }
        },
        "deprecated": false
      }
    },
    "/api/auth/register": {
      "post": {
        "tags": ["Users"],
        "summary": "Register account",
        "description": "Register an account",
        "operationId": "ApiAuthRegister_POST",
        "parameters": [],
        "requestBody": {
          "description": "",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/auth_schema"
              }
            }
          },
          "required": false
        },
        "responses": {
          "200": {
            "description": "OK",
            "headers": {},
            "content": {
              "application/json; charset=utf-8": {
                "schema": {
                  "type": "string",
                  "example": "All Good"
                }
              }
            }
          },
          "5XX": {
            "description": "Unexpected error."
          }
        },
        "deprecated": false
      }
    },
    "/api/auth/login": {
      "post": {
        "tags": ["Users"],
        "summary": "Login",
        "description": "Login to Landmarks app",
        "operationId": "ApiAuthLogin_POST",
        "parameters": [],
        "requestBody": {
          "description": "",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/auth_schema"
              }
            }
          },
          "required": false
        },
        "responses": {
          "200": {
            "description": "OK",
            "headers": {},
            "content": {
              "application/json; charset=utf-8": {
                "schema": {
                  "$ref": "#/components/schemas/ApiAuthLogin_POSTResponse"
                }
              }
            }
          },
          "5XX": {
            "description": "Unexpected error."
          }
        },
        "deprecated": false
      }
    }
  },
  "components": {
    "schemas": {
      "landmarks_page_schema": {
        "title": "landmarks_page_schema",
        "type": "object",
        "properties": {
          "docs": {
            "type": "object"
          },
          "totalDocs": {
            "type": "integer",
            "format": "int32"
          },
          "limit": {
            "type": "integer",
            "format": "int32"
          },
          "totalPages": {
            "type": "integer",
            "format": "int32"
          },
          "page": {
            "type": "integer",
            "format": "int32"
          },
          "pagingCounter": {
            "type": "integer",
            "format": "int32"
          },
          "hasPrevPage": {
            "type": "boolean"
          },
          "hasNextPage": {
            "type": "boolean"
          },
          "prevPage": {
            "type": "integer",
            "format": "int32"
          },
          "nextPage": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "landmark_schema": {
        "title": "landmark_schema",
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "imageUrlSet": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": ""
          },
          "description": {
            "type": "string"
          },
          "updatedAt": {
            "type": "string"
          },
          "comments": {
            "type": "array",
            "items": {
              "type": "object"
            },
            "description": ""
          }
        }
      },
      "comment_schema": {
        "title": "comment_schema",
        "type": "object",
        "properties": {
          "commentBody": {
            "type": "string"
          },
          "userEmail": {
            "type": "string"
          },
          "userName": {
            "type": "string"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "auth_schema": {
        "title": "auth_schema",
        "type": "object",
        "properties": {
          "password": {
            "type": "string"
          },
          "email": {
            "type": "string"
          }
        }
      },
      "ApiAuthLogin_POSTResponse": {
        "title": "ApiAuthLogin_POSTResponse",
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          }
        }
      },
      "Content-Type text/html": {
        "title": "Content-Type text/html",
        "enum": ["text/html; charset=utf-8"],
        "type": "string",
        "example": "text/html; charset=utf-8"
      },
      "Content-Type application/json": {
        "title": "Content-Type application/json",
        "enum": ["application/json"],
        "type": "string",
        "example": "application/json"
      }
    },
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "in": "header",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "BearerAuth": []
    }
  ],
  "tags": [
    {
      "name": "Landmarks",
      "description": "Operations with Landmarks"
